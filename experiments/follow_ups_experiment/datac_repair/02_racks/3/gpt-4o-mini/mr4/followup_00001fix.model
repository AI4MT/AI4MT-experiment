<tcinputCloud:DataCentre
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:tcinputCloud="http://tcinputCloud/1.0">
  <net bandwidth="10"
      latency="5">
    <switches/>
  </net>
  <racks numBoards="1">
    <board nodesPerBoard="8">
      <switch/>
      <nodeType
          CPUCores="4"
          CPUSpeed="11"
          MEMSpeed="11"
          DiskSpeed="17"/>
    </board>
  </racks>
  <racks numBoards="19">
    <board nodesPerBoard="16">
      <switch/>
      <nodeType
          CPUCores="13"
          CPUSpeed="16"
          MEMSpeed="5"
          DiskSpeed="12"/>
    </board>
  </racks>
</tcinputCloud:DataCentre>